{
  "name": "netless-app-monorepo",
  "private": true,
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@netless/window-manager": "^0.3.10",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.31",
    "@tsconfig/svelte": "^2.0.1",
    "@types/node": "^16.11.12",
    "@types/rollup-plugin-peer-deps-external": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.3",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "husky": "^7.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.1",
    "rimraf": "^3.0.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "sass": "^1.45.0",
    "svelte": "^3.44.3",
    "typescript": "^4.5.4",
    "vite": "^2.7.2",
    "white-web-sdk": "2.15.13"
  },
  "scripts": {
    "prettier": "prettier --write .",
    "lint": "eslint --ext .ts,.tsx,.svelte . && prettier --check .",
    "preinstall": "npx only-allow pnpm",
    "build-all": "pnpm run pre:build-all && pnpm build -r --filter ./packages",
    "build-all:dev": "pnpm run pre:build-all && pnpm build:dev -r --filter ./packages",
    "dev": "pnpm dev --filter playground",
    "pre:build-all": "run-s pre:build-shared pre:build-embedded-page-sdk",
    "pre:build-shared": "pnpm build --filter app-shared",
    "pre:build-embedded-page-sdk": "pnpm build --filter app-embedded-page-sdk"
  }
